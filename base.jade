include mixins

doctype html
html
	head
		block head
			block head-meta
				meta(charset="utf-8")
				meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
				meta(name="viewport", content="width=device-width, initial-scale=1")

			+html5shiv

			block head-title
			block head-assets

	body
		mixin browsehappy
		block body
		block after-body
