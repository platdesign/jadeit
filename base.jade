include mixins


- var page = {og:{}};
mixin setPage(attrs)
	- for(var i in attrs) {
	- page[i] = attrs[i]
	- }
block base
	


doctype html
html
	head
		block head
			meta(charset="utf-8")
			meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
			meta(name="viewport", content="width=device-width, initial-scale=1")
			
			title= page.title
			+page-meta(page)

			+conditionalComment('if lt IE 9')
				+js("http://html5shiv.googlecode.com/svn/trunk/html5.js")


			block head-js
			block head-css

	body
		block body
		

	block page-footer